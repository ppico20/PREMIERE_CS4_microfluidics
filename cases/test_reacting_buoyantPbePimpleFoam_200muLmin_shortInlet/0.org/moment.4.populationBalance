FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      moment.4.populationBalance;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 0 0 0 0 0];

internalField   uniform 5.0e-11;

boundaryField
{
    inlet1
    {
        type            inletOutlet;
        inletValue      $internalField;
    }

    inlet2
    {
        type            inletOutlet;
        inletValue      $internalField;
    }

    walls
    {
        type            zeroGradient;
    }

    outlet
    {
        type            zeroGradient;
    }

    frontBack
    {
        type            empty;
    }
}


