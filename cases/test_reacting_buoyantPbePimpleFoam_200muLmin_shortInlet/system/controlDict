FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      controlDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

application     reacting_buoyantPbePimpleFoam;

startFrom       latestTime;

startTime       0;

stopAt          endTime;

endTime         80;

deltaT          1e-5;

writeControl    runTime;

writeInterval   1e-3;

purgeWrite      0;

writeFormat     ascii;

writePrecision  6;

writeCompression off;

timeFormat      general;

timePrecision   6;

runTimeModifiable yes;

adjustTimeStep  true;

maxCo           0.5;

maxAlphaCo      0.3;

maxDeltaT       1;


libs
(
"libcompressible.so"
);


functions{

     components1
     {
         type            components;
         libs            (fieldFunctionObjects);
         field           U;
        executeControl      runTime;
        executeInterval     0.2;
        writeControl        runTime;
        writeInterval       0.2;
     }

    volAverage
    {
        type            volFieldValue;
        libs            ("libfieldFunctionObjects.so");
        fields         
        (
            y_AgNO3
            y_tri
            y_tannic
            y_H2O
            y_Agl
            y_Ags
            y_Ags2
            moment.0.populationBalance
            moment.1.populationBalance
            moment.2.populationBalance
            moment.3.populationBalance
            Ux
            Uy
            p
        );

        writeFields         no;
        name                volumeAverage;
        operation	    volAverage;
        executeControl      runTime;
        executeInterval     0.2;
        writeControl        runTime;
        writeInterval       0.2;
    }

    probes1
    {
        functionObjectLibs ( "libsampling.so" );
        type               probes;
        name               probes1;
        executeControl      runTime;
        executeInterval     0.2;
        writeControl        runTime;
        writeInterval       0.2;
        fields
        (
            y_AgNO3
            y_tri
            y_tannic
            y_H2O
            y_Agl
            y_Ags
            y_Ags2
            moment.0.populationBalance
            moment.1.populationBalance
            moment.2.populationBalance
            moment.3.populationBalance
            rate_product_Agl_reduction
            rate_product_Ags_nuc
            rate_product_Ags2_g
            Ux
            Uy
            p
         );

        probeLocations
        (
            (0.00225 1.9995 0.00025)
        );
    }

    probes2
    {
        functionObjectLibs ( "libsampling.so" );
        type               probes;
        name               probes2;
        executeControl      runTime;
        executeInterval     0.2;
        writeControl        runTime;
        writeInterval       0.2;
        fields
        (
            y_AgNO3
            y_tri
            y_tannic
            y_H2O
            y_Agl
            y_Ags
            y_Ags2
            moment.0.populationBalance
            moment.1.populationBalance
            moment.2.populationBalance
            moment.3.populationBalance
            rate_product_Agl_reduction
            rate_product_Ags_nuc
            rate_product_Ags2_g
            Ux
            Uy
            p
         );

        probeLocations
        (
            (0.00225 0.0005 0.00025)
        );
    }

}



