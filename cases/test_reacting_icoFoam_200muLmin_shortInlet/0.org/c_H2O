FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      c_H2O;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 -3 0 0 0 0 0];

internalField   uniform 1000;

boundaryField
{
    inlet1
    {
        type            fixedValue;
        value           uniform 999.8437169;
    }

    inlet2
    {
        type            fixedValue;
        value           uniform 998.2091767;
    }

    walls
    {
        type            zeroGradient;
    }

    outlet
    {
        type            zeroGradient;
    }

    frontBack
    {
        type            empty;
    }
}

